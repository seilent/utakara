# Admin credentials (optional, defaults to "admin")
ADMIN_USERNAME=
ADMIN_PASSWORD=

# Required for NextAuth.js session encryption
NEXTAUTH_SECRET=

# Required for YouTube API (for auto-searching songs)
YOUTUBE_API_KEY=

# Audio storage configuration (optional, defaults to "local")
# Can be "local" or "proxy"
AUDIO_STORAGE=local

# Proxy URL for audio streaming (required if AUDIO_STORAGE=proxy)
AUDIO_PROXY_URL=https://your-proxy-url.com